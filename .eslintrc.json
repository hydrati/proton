{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard-with-typescript",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json", "tsconfig.node.json", "tsconfig.vitest.json"]
    },
    "rules": {
        "prettier/prettier": ["warn"]
    }
}
